{
  "name": "Firewatch Light Theme",
  "type": "light",
  "colors": {
    "background": "#faf7f4",
    "foreground": "#886624",
    "input.background": "#faf7f4",
    "input.foreground": "#886624",
    "input.border": "#484a534C",
    "editor.background": "#faf7f4",
    "editor.foreground": "#886624",
    "editor.selectionBackground": "#e5dcca",
    "panel.background": "#faf7f4",
    "sideBar.background": "#faf7f4",
    "sideBar.border": "#f0eeeb",
    "caret": "#e35c93",
    "invisibles": "#773e22",
    "guide": "#dde5ff",
    "hoverHighlight": "#00a7c0",
    "referenceHighlight": "#cd9500",
    "lineHighlight": "#616571",
    "rangeHighlight": "#eaca8d",
    "selection": "#e5dcca",
    "inactiveSelection": "#fbfbfb",
    "selectionHighlight": "#616571",
    "findRangeHighlight": "#818696",
    "findMatchHighlight": "#eaca8d",
    "currentFindMatchHighlight": "#9297aa",
    "wordHighlight": "#676b78",
    "wordHighlightStrong": "#0096c8",
    "activeLinkForeground": "#0096c8",
    "gotoDefinitionLinkForeground": "#0096c8",
    "statusBar.foreground": "#886624",
    "statusBar.background": "#faf7f4",
    "statusBar.border": "#484a53",
    "statusBar.noFolderBackground": "#fbfbfb",
    "statusBar.debuggingBackground": "#fbfbfb",
    "statusBar.debuggingForeground": "#886624",
    "statusBarItem.activeBackground": "#faf7f4",
    "statusBarItem.hoverBackground": "#fbfbfb",
    "statusBarItem.prominentBackground": "#faf7f4",
    "statusBarItem.prominentHoverBackground": "#fbfbfb"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#5C6370"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#886624"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#eaca8d"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#65c1cd"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#65c1cd"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "#e78287",
        "fontStyle": ""
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#eb6d5a"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#eb6d5a",
        "fontStyle": ""
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "foreground": "#eb6d5a"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.class",
      "settings": {
        "foreground": "#eaca8d"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic underline",
        "foreground": "#886624"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#676b78"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#eaca8d"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "",
        "foreground": "#e78287"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "",
        "foreground": "#eaca8d"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#65c1cd"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "#eaca8d"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings":{ "foreground": "#c8a35a" }
    }, 
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c8a35a"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "foreground": "#886624"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "background": "#eb6d5a",
        "fontStyle": "",
        "foreground": "#ff8f95"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "background": "#e5dcca",
        "foreground": "#484a53"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#eaca8d"
      }
    },
    {
      "name": "Markup Bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#eaca8d",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#eaca8d",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#886624"
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "#d190e3"
      }
    }
  ]
}
